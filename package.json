{
  "name": "almacenes-galpones-espacios-libres",
  "version": "1.0.0",
  "description": "Plataforma web para intermediacion tecnologica de alquiler temporal de almacenes, galpones y espacios libres en Bolivia",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node server/index.js",
    "server": "node server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "setup": "npm install && cd client && npm install"
  },
  "keywords": [
    "almacenes",
    "galpones",
    "espacios",
    "bolivia",
    "alquiler"
  ],
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.22.1",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.3.1",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.3",
    "leaflet": "^1.9.4",
    "multer": "^1.4.5-lts.1",
    "pdfkit": "^0.14.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.11.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
